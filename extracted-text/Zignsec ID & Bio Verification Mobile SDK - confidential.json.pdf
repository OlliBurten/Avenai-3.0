      "text": "const api = new DocumentReaderApi({ \n    basePath: \"https://test-gateway.zignsec.com/mobilesdk/docreader\", \n    baseOptions: {  \n        headers: { \n            Authorization: `Bearer ${accessToken}`, \n            \"Zs-Product-Key\": subscriptionKey, \n        } \n    }, \n});",
